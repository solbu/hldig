# Make sure all data has been copied from the source directory
#
test_prog=$1

[ ! -d conf ] && cp -rp $srcdir/conf .
[ ! -d htdocs ] && cp -rp $srcdir/htdocs .

if [ ! -f skiptest_db.txt ] ; then 
	cp $srcdir/skiptest_db.txt .
fi

exec $srcdir/$test_prog
